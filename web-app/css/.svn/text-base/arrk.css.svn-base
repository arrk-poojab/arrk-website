	padding: 0;
}

.download-container ul li {
	background: url("../images/separator.png") repeat-x scroll left bottom
		transparent;
	list-style-type: none;
	margin: 0;
	padding: 8px 0 15px 5px;
}

#homeSlider .homeslider-pauseplay {
	display: none;
}

.phone-menu {
	display: none;
}

#domain .jcarousel-skin-tango .jcarousel-item-horizontal{
	background-attachment: scroll;
    background-clip: border-box;
    background-color: transparent;
    background-image: url("../images/navSeparator.png");
    background-origin: padding-box;
    background-position: right top;
    background-repeat: repeat-y;
    background-size: 1px 28px;
    padding: 0 10px;
    width: 280px !important;
}

#domain .jcarousel-skin-tango .jcarousel-container-horizontal {
    padding: 20px 2% 0;
    width: 96%;
	margin: 0 auto;
}
#domain .jcarousel-skin-tango .jcarousel-item-horizontal {
	margin-left: 0;
    margin-right: 10px;
}
#domainExpertise .jcarousel-skin-tango .jcarousel-item-horizontal{
	background-attachment: scroll;
    background-clip: border-box;
    background-color: transparent;
    background-image: url("../images/navSeparator.png");
    background-origin: padding-box;
    background-position: right top;
    background-repeat: repeat-y;
    background-size: 1px 28px;
    padding: 0 10px;
    width: 220px !important;
}
.content #domainExpertise h3{
	font-size: 16px;
}

